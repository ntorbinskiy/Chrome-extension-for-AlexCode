{
	"name": "Todoist extension",
	"description": "Todoist extension",
	"version": "1.0",
	"content_scripts": [
		{
			"matches": [
				"https://todoist.com/app/*"
			],
			"runat": "documet-idle",
			"js": [
				"content.bundle.js"
			]
		}
	],
	"web_accessible_resources": [
		{
			"resources": [
				"icon.png"
			],
			"matches": [
				"<all_urls>"
			]
		}
	],
	"manifest_version": 3
}